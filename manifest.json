{
  "manifest_version": 3,
  "name": "Youtube Ad Blocker",
  "description": "Youtube ads blocker extension",
  "version": "1.0.0",
  "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "run_at": "document_idle",
      "matches": [
        "*://*.youtube.com/*",
        "*://*.youtu.be/*"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "content_security_policy": {}
}